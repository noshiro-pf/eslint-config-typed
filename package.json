{
  "name": "eslint-config-typed",
  "version": "4.6.0",
  "private": false,
  "keywords": [
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noshiro-pf/eslint-config-typed.git"
  },
  "license": "Apache-2.0",
  "author": "noshiro-pf <noshiro.pf@gmail.com>",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types.d.mts",
        "default": "./dist/entry-point.mjs"
      }
    },
    "./vitest-globals": {
      "types": "./vitest-globals.d.ts"
    }
  },
  "module": "./dist/entry-point.mjs",
  "types": "./dist/types.d.mts",
  "files": [
    "src",
    "dist",
    "README.md",
    "LICENSE",
    "vitest-globals.d.ts"
  ],
  "scripts": {
    "apply-type-transformations": "tsx ./scripts/gen-eslint-rules/codemod/apply-type-transformations.mts",
    "build": "tsx ./scripts/cmd/build.mts",
    "build:min": "tsx ./scripts/cmd/build.mts --skip-check",
    "check-all": "tsx ./scripts/cmd/check-all.mts",
    "check:ext": "tsx ./scripts/cmd/check-ext.mts",
    "cspell": "cspell \"**\" --gitignore --gitignore-root ./ --no-progress",
    "doc": "tsx ./scripts/cmd/gen-docs.mts",
    "doc:embed": "tsx ./scripts/cmd/embed-samples.mts",
    "fmt": "format-uncommitted",
    "fmt:diff": "format-diff-from origin/main",
    "fmt:full": "prettier --write .",
    "gen-rule-type": "tsx ./scripts/gen-eslint-rules/main.mts",
    "gh:apply-all": "gh-apply-all",
    "gh:apply-repository-settings": "gh-apply-repository-settings",
    "gh:apply-rulesets": "gh-apply-rulesets",
    "gh:apply-variables": "gh-apply-variables",
    "gh:backup-all": "gh-backup-all",
    "gh:backup-repository-settings": "gh-backup-repository-settings",
    "gh:backup-rulesets": "gh-backup-rulesets",
    "gi": "run-s gi:src fmt",
    "gi:src": "tsx scripts/cmd/gen-index.mts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "md": "markdownlint-cli2",
    "test": "pnpm run z:vitest:node run",
    "test:cov": "pnpm run z:vitest:node run --coverage",
    "test:cov:ui": "vite preview --outDir ./coverage",
    "test:ui": "pnpm run z:vitest:node --ui",
    "testw": "pnpm run z:vitest:node watch",
    "tsc": "tsc --noEmit",
    "tscw": "tsc --noEmit --watch -p ./tsconfig.json",
    "type-check": "tsc --noEmit",
    "update-packages": "pnpm update --latest",
    "z:vitest": "vitest --config ./configs/vitest.config.mts",
    "z:vitest:node": "pnpm run z:vitest --project='Node.js'"
  },
  "dependencies": {
    "@eslint/js": "9.39.2",
    "@sindresorhus/is": "^7.2.0",
    "@stylistic/eslint-plugin": "^5.7.0",
    "@types/eslint": "9.6.1",
    "@typescript-eslint/eslint-plugin": "8.53.0",
    "@typescript-eslint/parser": "8.53.0",
    "@typescript-eslint/type-utils": "8.53.0",
    "@typescript-eslint/utils": "8.53.0",
    "@vitest/eslint-plugin": "^1.6.6",
    "eslint": "9.39.2",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-array-func": "5.1.0",
    "eslint-plugin-cypress": "5.2.1",
    "eslint-plugin-eslint-plugin": "7.3.0",
    "eslint-plugin-functional": "9.0.2",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jest": "29.12.1",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-math": "^0.13.1",
    "eslint-plugin-n": "^17.23.2",
    "eslint-plugin-playwright": "2.5.0",
    "eslint-plugin-prefer-arrow-functions": "3.9.1",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-react-perf": "3.3.3",
    "eslint-plugin-react-refresh": "0.4.26",
    "eslint-plugin-security": "3.0.1",
    "eslint-plugin-sort-destructure-keys": "^2.0.0",
    "eslint-plugin-testing-library": "7.15.4",
    "eslint-plugin-unicorn": "62.0.0",
    "globals": "17.0.0",
    "is-glob": "^4.0.3",
    "is-immutable-type": "5.0.1",
    "micromatch": "^4.0.8",
    "ts-data-forge": "^6.2.1",
    "tsutils": "3.21.0",
    "typescript-eslint": "8.53.0"
  },
  "devDependencies": {
    "@eslint/core": "1.0.1",
    "@rollup/plugin-replace": "6.0.3",
    "@rollup/plugin-strip": "3.0.4",
    "@rollup/plugin-typescript": "12.3.0",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "13.0.1",
    "@semantic-release/exec": "7.1.0",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "12.0.2",
    "@semantic-release/npm": "13.1.3",
    "@semantic-release/release-notes-generator": "14.1.0",
    "@types/estree": "1.0.8",
    "@types/is-glob": "4.0.4",
    "@types/json-schema": "7.0.15",
    "@types/micromatch": "4.0.10",
    "@types/node": "25.0.10",
    "@types/react": "19.2.9",
    "@typescript-eslint/rule-tester": "8.53.0",
    "@vitest/coverage-v8": "4.0.17",
    "@vitest/ui": "4.0.17",
    "conventional-changelog-conventionalcommits": "9.1.0",
    "cspell": "9.6.0",
    "dedent": "1.7.1",
    "fast-glob": "3.3.3",
    "github-settings-as-code": "1.0.12",
    "immer": "^11.1.3",
    "jiti": "2.6.1",
    "json-schema": "0.4.0",
    "json-schema-to-typescript": "15.0.4",
    "markdownlint": "0.40.0",
    "markdownlint-cli2": "0.20.0",
    "npm-run-all2": "8.0.4",
    "prettier": "3.8.0",
    "prettier-plugin-jsdoc": "1.8.0",
    "prettier-plugin-organize-imports": "4.3.0",
    "prettier-plugin-packagejson": "2.5.21",
    "react": "19.2.3",
    "rollup": "4.55.1",
    "semantic-release": "25.0.2",
    "ts-fortress": "6.2.3",
    "ts-morph": "27.0.2",
    "ts-repo-utils": "8.0.3",
    "ts-type-forge": "2.3.0",
    "tslib": "2.8.1",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "vitest": "4.0.17"
  },
  "peerDependencies": {
    "eslint": ">=9.0.0",
    "typescript": ">=5.0.0"
  },
  "packageManager": "pnpm@10.28.1",
  "engines": {
    "node": ">=20.11.0"
  },
  "volta": {
    "node": "25.2.1"
  }
}
